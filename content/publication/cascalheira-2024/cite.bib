@article{Cascalheira2024,
 abstract = {During the summer of 2022, monkeypox (mpox) became a public health emergency primarily affecting members of sexual and gender minority (SGM) communities. Understanding how SGM social media users perceived mpox could inform community-based public health prevention strategies for future disease outbreaks. However, social media platform selection could substantially affect the insights drawn about mpox.},
 author = {Cory J. Cascalheira and Kelsey Corro and Chenglin Hong and Taylor K. Rohleen and Ollie Trac and Mehrab Beikzadeh and Jillian R. Scheer and Shah Muhammad Hamdi and Soukaina Filali Boubrahimi and Ian W. Holloway},
 doi = {10.1007/s13178-024-01058-4},
 issn = {1553-6610},
 journal = {Sexuality Research and Social Policy},
 title = {An Analysis of Mpox Communication on Reddit vs Twitter During the 2022 Mpox Outbreak},
 url = {https://doi.org/10.1007/s13178-024-01058-4},
 year = {2024}
}
